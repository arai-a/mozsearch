<table class="symbol-tree-table">
{% if table.columns != empty %}
  <thead>
    <tr>
    {% for col in table.columns %}
      {% include 'query_results/table_header_cell.liquid' col: col %}
    {% endfor %}
    </tr>
    {% if table.sub_columns.size > 0 %}
      <tr>
      {% for col in table.sub_columns %}
        {% include 'query_results/table_header_cell.liquid' col: col %}
      {% endfor %}
      </tr>
    {% endif %}
  </thead>
{% endif %}
  <tbody>
    {%- for kid in table.rows -%}
      {%- include 'query_results/symbol_tree_table_node.liquid' node: kid -%}
    {%- endfor -%}
  </tbody>
</table>
